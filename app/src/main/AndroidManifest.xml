<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cattyperi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdoptionActivity"></activity>
        <activity android:name=".DonationActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".NewsActivity"></activity>
        <activity android:name=".RegisterUserActivity"></activity>
        <activity android:name=".RegisterCatActivity"></activity>
        <activity android:name=".AdoptionTranscationActivity"></activity>
        <activity android:name=".DonationTransactionActivity"></activity>
    </application>

</manifest>
