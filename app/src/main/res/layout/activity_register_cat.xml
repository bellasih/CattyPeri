    <EditText
        android:id="@+id/name_cat"
        android:layout_width="177dp"
        android:layout_height="34dp"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="228dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
